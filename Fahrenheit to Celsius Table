import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
        int S , E, W , fah;
        S = sc.nextInt();
        E = sc.nextInt();
        W = sc.nextInt();
        while(S<=E){
            fah = ((5*(S-32))/9);
            System.out.println(S+ " " +fah);
            S+=W;
        }
	}

}
